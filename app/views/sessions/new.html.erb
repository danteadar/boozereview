<h1>Login</h1> <br><br>
<%= form_tag '/login', method: :post do %>
  <div>
    <%= label_tag :email %>
    <%= email_field_tag :email %>
  </div>
  <br>
  <div>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
  <br>
  <%= submit_tag "Submit" %>
<% end %>
