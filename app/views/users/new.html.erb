<div class="show">
  <div class="site-wrapper-inner">
  <br><br><br><br><br>

    <h1>Sign Up:</h1><br><br>

    <%= form_tag "/users/", multipart: true, method: :post do  %>
      <div>
        <%= text_field_tag :first_name, nil, :placeholder => "First Name" %>
      </div>
      <br>
      <div>
        <%= text_field_tag :last_name, nil, :placeholder => "Last Name" %>
      </div>
      <br>
      <div>
        <%= email_field_tag :email, nil, :placeholder => "Email" %>
      </div>
      <br>
      <div>
        <%= text_field_tag :user_name, nil, :placeholder => "User Name" %>
      </div>
        <br>
      <div>
        <%= password_field_tag :password, nil, :placeholder => "Password" %>
      </div>
      <br>
      <div>
        <%= password_field_tag :password_confirmation, nil, :placeholder => "Confirm Password" %>
      </div>
      <br>
      <%= submit_tag "Submit" %>
    <% end %>
    <br><br><br><br><br><br><br><br>

  </div>
</div>